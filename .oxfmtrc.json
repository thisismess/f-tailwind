{
    "$schema": "./node_modules/oxfmt/configuration_schema.json",
    "printWidth": 180,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "trailingComma": "es5",
    "proseWrap": "always",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": true,
    "endOfLine": "lf",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "singleAttributePerLine": false
}
